{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "HYMetaLab Desktop",
  "version": "0.1.0",
  "identifier": "com.hymetalab.desktop",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "label": "main",
        "title": "HYMetaLab Launcher",
        "width": 460,
        "height": 620,
        "minWidth": 460,
        "minHeight": 620,
        "resizable": false,
        "maximizable": false,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
